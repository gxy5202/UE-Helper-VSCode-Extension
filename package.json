{
	"name": "ue-helper",
	"displayName": "UE Helper",
	"description": "a vscode extension for ue developer",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.excelToJson",
		"onCommand:extension.modelSettingsParser"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.excelToJson",
				"title": "Excel to json"
			},
			{
				"command": "extension.modelSettingsParser",
				"title": "Model settings parser"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "extension.excelToJson",
					"group": "navigation"
				},
				{
					"when": "explorerResourceIsFolder",
					"command": "extension.modelSettingsParser",
					"group": "navigation"
				}
			]
		},
		"keybindings": [
			{
				"command": "extension.excelToJson",
				"key": "ctrl+f1",
				"mac": "cmd+f1",
				"when": "slideBarFocus && activeViewlet == 'workbench.view.explorer'"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"xlsx": "^0.16.9"
	}
}
